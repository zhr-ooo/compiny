at.$message.success(res.message);\n                that.$emit('ok');\n              }else{\n                that.$message.warning(res.message);\n              }\n            }).finally(() => {\n              that.confirmLoading = false;\n              that.close();\n            })\n\n\n\n          }\n        })\n      },\n      handleCancel () {\n        this.close()\n      },\n\n\n    }\n  }\n</script>\n\n<style lang=\"less\" scoped>\n\n</style>"],"sourceRoot":"src/views/platform/modules"}]}