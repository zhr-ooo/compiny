AA;AACA,MAAA,YAAA,CAAA,IAAA,CAAA,GAAA,CAAA,MAAA,EAAA;AAAA,QAAA,EAAA,EAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,OAAA,EAAA;AACA,UAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,UAAA,IAAA,CAAA,QAAA;AACA,SAHA,MAGA;AACA,UAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,OAPA;AAQA;AAjBA;AAzEA,CAAA","sourcesContent":["<template>\r\n  <a-card :bordered=\"false\">\r\n    <div class=\"table-page-search-wrapper\">\r\n      <a-form layout=\"inline\">\r\n        <a-row :gutter=\"24\">\r\n          <a-col :md=\"4\" :sm=\"4\">\r\n            <a-form-item label=\"角色\">\r\n              <a-select placeholder=\"请选择角色\" v-model=\"queryParam.role\">\r\n                <a-select-option :value=\"1\">VIP</a-select-option>\r\n                <a-select-option :value=\"2\">创客达人</a-select-option>\r\n                <a-select-option :value=\"3\">大使服务商</a-select-option>\r\n              </a-select>\r\n            </a-form-item>\r\n          </a-col>\r\n          <a-col :md=\"3\" :sm=\"3\">\r\n            <span\r\n              style=\"float: left;overflow: hidden;margin-left:8px\"\r\n              class=\"table-page-search-submitButtons\"\r\n            >\r\n              <a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button\r\n                type=\"primary\"\r\n                @click=\"searchReset\"\r\n                icon=\"reload\"\r\n                style=\"margin-left: 8px\"\r\n              >重置</a-button>\r\n            </span>\r\n          </a-col>\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\r\n    </div>\r\n    <!-- table区域-begin -->\r\n    <div>\r\n      <a-table\r\n        ref=\"table\"\r\n        size=\"middle\"\r\n        bordered\r\n        rowKey=\"id\"\r\n        :columns=\"columns\"\r\n        :dataSource=\"dataSource\"\r\n        :pagination=\"ipagination\"\r\n        :loading=\"loading\"\r\n        @change=\"handleTableChange\"\r\n      >\r\n        <span slot=\"action\" slot-scope=\"text, record\">\r\n          <a @click=\"handleEdit(record)\">编辑</a>\r\n          <a-divider type=\"vertical\" />\r\n          <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record)\">\r\n            <a style=\"color: red\">删除</a>\r\n          </a-popconfirm>\r\n        </span>\r\n        <template slot=\"rebate\" slot-scope=\"text, record\">\r\n          <span slot=\"rebate\">{{ record.vipRatio}}%</span>\r\n        </template>\r\n        <template slot=\"rebate1\" slot-scope=\"text, record\">\r\n          <span slot=\"rebate1\">{{ record.creatorRatio}}%</span>\r\n        </template>\r\n        <template slot=\"rebate2\" slot-scope=\"text, record\">\r\n          <span slot=\"rebate2\">{{ record.ambassadorRatio}}%</span>\r\n        </template>\r\n      </a-table>\r\n    </div>\r\n    <!-- table区域-end -->\r\n\r\n    <!-- 表单区域 -->\r\n    <Provider-modals ref=\"modalForm\" @ok=\"modalFormOk\"></Provider-modals>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\nimport ProviderModals from \"./modules/ProviderModals\";\r\nimport { postAction, deleteAction, putAction } from \"@/api/manage\";\r\nimport { JeecgListMixin } from \"@/mixins/JeecgListMixin\";\r\nimport JEllipsis from \"@/components/jeecg/JEllipsis\";\r\n\r\nexport default {\r\n  name: \"ProviderList\",\r\n  mixins: [JeecgListMixin],\r\n  components: {\r\n    JEllipsis,\r\n    ProviderModals\r\n  },\r\n  data() {\r\n    return {\r\n      description: \"服务商分销比例列表\",\r\n      fenxiao: true,\r\n      vipsorter: {\r\n        order: \"desc\"\r\n      },\r\n      // 表头\r\n      columns: [\r\n        {\r\n          title: \"角色\",\r\n          align: \"center\",\r\n          dataIndex: \"role\",\r\n          customRender: function(text) {\r\n            if (text == \"1\") {\r\n              return \"VIP\";\r\n            } else if (text == \"2\") {\r\n              return \"创客达人\";\r\n            } else if (text == \"3\") {\r\n              return \"大使服务商\";\r\n            }\r\n          }\r\n        },\r\n        {\r\n          title: \"星级\",\r\n          align: \"center\",\r\n          dataIndex: \"level\"\r\n        },\r\n        {\r\n          title: \"VIP分销\",\r\n          align: \"center\",\r\n          dataIndex: \"vipRatio\",\r\n          scopedSlots: { customRender: \"rebate\" }\r\n        },\r\n        {\r\n          title: \"创客达人分销\",\r\n          align: \"center\",\r\n          dataIndex: \"creatorRatio\",\r\n          scopedSlots: { customRender: \"rebate1\" }\r\n        },\r\n        {\r\n          title: \"大使服务商分销\",\r\n          align: \"center\",\r\n          dataIndex: \"ambassadorRatio\",\r\n          scopedSlots: { customRender: \"rebate2\" }\r\n        },\r\n\r\n        {\r\n          title: \"升级数量\",\r\n          align: \"center\",\r\n          dataIndex: \"upgradeNum\"\r\n        },\r\n\r\n        {\r\n          title: \"操作\",\r\n          dataIndex: \"action\",\r\n          align: \"center\",\r\n          scopedSlots: { customRender: \"action\" }\r\n        }\r\n      ],\r\n      url: {\r\n        list: \"/system/service/distribution/list\",\r\n        delete: \"/system/service/distribution/delete\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    handleDelete: function(record) {\r\n      console.log(record);\r\n      var id = record.id;\r\n      if (!this.url.delete) {\r\n        this.$message.error(\"请设置url.delete属性!\");\r\n        return;\r\n      }\r\n      var that = this;\r\n      deleteAction(that.url.delete, { id: id }).then(res => {\r\n        if (res.success) {\r\n          that.$message.success(res.message);\r\n          that.loadData();\r\n        } else {\r\n          that.$message.warning(res.message);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n@import \"~@assets/less/common.less\";\r\n</style>"],"sourceRoot":"src/views/platform"}]}