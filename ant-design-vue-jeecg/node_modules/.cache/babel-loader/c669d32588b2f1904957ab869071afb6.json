 groupName: '',\r\n          type: null\r\n        },\r\n        param: {\r\n          groupName: '',\r\n          type: null\r\n        },\r\n        // 表头\r\n        columns: [\r\n          {\r\n            title: '群名称',\r\n            align: \"center\",\r\n            dataIndex: 'name'\r\n          },\r\n          {\r\n            title: '发言人',\r\n            align: \"center\",\r\n            dataIndex: 'nickName'\r\n          },\r\n          {\r\n            title: '消息类型',\r\n            align: \"center\",\r\n            dataIndex: 'meaasgeType',\r\n            customRender: function(text) {\r\n              if (text === 0) {\r\n                return '文本'\r\n              } else if (text === 2) {\r\n                return '音频'\r\n              } else if (text === 3) {\r\n                return '图片'\r\n              } else if (text === 4) {\r\n                return '文件'\r\n              } else if (text === 5) {\r\n                return '视频'\r\n              }\r\n            }\r\n          },\r\n          {\r\n            title: '消息内容',\r\n            align: \"center\",\r\n            dataIndex: 'meaasgeContent',\r\n            scopedSlots: {customRender: 'templateContent'},\r\n          },\r\n          {\r\n            title: '发言时间',\r\n            align: \"center\",\r\n            dataIndex: 'msgtime'\r\n          },\r\n        ],\r\n        url: {\r\n          list: \"/system/groupHisMessage/list\",\r\n          exportXlsUrl: \"message/sysMessageTemplate/exportXls\",\r\n          importExcelUrl: \"message/sysMessageTemplate/importExcel\",\r\n        },\r\n      }\r\n    },\r\n    computed: {},\r\n    methods: {\r\n      handleTest(record){\r\n        this.$refs.testModal.open(record);\r\n        this.$refs.testModal.title = \"发送测试\";\r\n      }\r\n\r\n    }\r\n  }\r\n</script>\r\n<style lang=\"less\" scoped>\r\n  /** Button按钮间距 */\r\n  .ant-btn {\r\n    margin-left: 3px\r\n  }\r\n  \r\n  .ant-card-body .table-operator {\r\n    margin-bottom: 18px;\r\n  }\r\n