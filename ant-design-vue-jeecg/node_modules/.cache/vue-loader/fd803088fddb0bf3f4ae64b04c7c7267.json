a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button\r\n                type=\"primary\"\r\n                @click=\"searchReset\"\r\n                icon=\"reload\"\r\n                style=\"margin-left: 8px\"\r\n              >重置</a-button>\r\n            </span>\r\n          </a-col>\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n    <!--  分润 -->\r\n    <!-- 表格区域 -->\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\r\n    </div>\r\n    <div>\r\n      <a-table\r\n        ref=\"table\"\r\n        size=\"middle\"\r\n        bordered\r\n        rowKey=\"id\"\r\n        :columns=\"columns\"\r\n        :dataSource=\"dataSource\"\r\n        :pagination=\"ipagination\"\r\n        :loading=\"loading\"\r\n        @change=\"handleTableChange\"\r\n      >\r\n        <span slot=\"header\" slot-scope=\"text\">\r\n          <a-avatar\r\n            shape=\"square\"\r\n            size=\"large\"\r\n            :src=\"text\"\r\n            :srcSet=\"text\"\r\n            @click.stop=\"bigImg(text)\"\r\n          ></a-avatar>\r\n        </span>\r\n        <span slot=\"action\" slot-scope=\"text, record\" @click.stop>\r\n          <a @click.stop=\"handleEdit(record)\">编辑</a>\r\n          <a-divider type=\"vertical\" />\r\n          <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record)\">\r\n            <a style=\"color: red\">删除</a>\r\n          </a-popconfirm>\r\n        </span>\r\n      </a-table>\r\n    </div>\r\n    <!-- 表单区域 -->\r\n    <!--  图片点击放大  -->\r\n    <product-new-big-img ref=\"bigImg\"></product-new-big-img>\r\n    <express-modals ref=\"modalForm\" @ok=\"modalFormOk\"></express-modals>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\nimport { postAction, deleteAction,getAction } from \"@/api/manage\";\r\nimport ProductNewBigImg from \"../product/modules/ProductNewBigImg\";\r\nimport { JeecgListMixin } from \"@/mixins/