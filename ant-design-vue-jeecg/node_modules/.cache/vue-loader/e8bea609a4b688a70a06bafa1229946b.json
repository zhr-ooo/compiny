:bordered=\"false\">\n\n    <!-- 查询区域 -->\n    <div class=\"table-page-search-wrapper\">\n      <a-form layout=\"inline\">\n        <a-row :gutter=\"24\">\n\n          <a-col :md=\"6\" :sm=\"8\">\n            <a-form-item label=\"当前登陆的用户ID\">\n              <a-input placeholder=\"请输入当前登陆的用户ID\" v-model=\"queryParam.userid\"></a-input>\n            </a-form-item>\n          </a-col>\n          <a-col :md=\"6\" :sm=\"8\">\n            <a-form-item label=\"转发的用户ID\">\n              <a-input placeholder=\"请输入转发的用户ID\" v-model=\"queryParam.forwarduserid\"></a-input>\n            </a-form-item>\n          </a-col>\n        <template v-if=\"toggleSearchStatus\">\n        <a-col :md=\"6\" :sm=\"8\">\n            <a-form-item label=\"产品ID\">\n              <a-input placeholder=\"请输入产品ID\" v-model=\"queryParam.productid\"></a-input>\n            </a-form-item>\n          </a-col>\n          <a-col :md=\"6\" :sm=\"8\">\n            <a-form-item label=\"创建时间\">\n              <a-input placeholder=\"请输入创建时间\" v-model=\"queryParam.createtime\"></a-input>\n            </a-form-item>\n          </a-col>\n          <a-col :md=\"6\" :sm=\"8\">\n            <a-form-item label=\"类型（0转发注册1转发进入）\">\n              <a-input placeholder=\"请输入类型（0转发注册1转发进入）\" v-model=\"queryParam.type\"></a-input>\n            </a-form-item>\n          </a-col>\n          </template>\n          <a-col :md=\"6\" :sm=\"8\" >\n            <span style=\"float: left;overflow: hidden;\" class=\"table-page-search-submitButtons\">\n              <a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\n              <a-button type=\"primary\" @click=\"searchReset\" icon=\"reload\" style=\"margin-left: 8px\">重置</a-button>\n              <a @click=\"handleToggleSearch\" style=\"margin-left: 8px\">\n                {{ toggleSearchStatus ? '收起' : '展开' }}\n                <a-icon :type=\"toggleSearchStatus ? 'up' : 'down'\"/>\n              </a>\n            </span>\n          </a-col>\n\n        </a-row>\n      </a-form>\n    </div>\n\n    <!-- 操作按钮区域 -->\n    <div class=\"table-operator\">\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\n      <a-button type=\"primary\" icon=\"download\" @click=\"handleExportXls('产品转发表')\">导出</a-button>\n      <a-upload name=\"file\" :showUploadList=\"false\" :multiple=\"false\" :headers=\"tokenHeader\" :action=\"importExcelUrl\" @change=\"handleImportExcel\">\n        <a-button type=\"primary\" icon=\"import\">导入</a-button>\n      </a-upload>\n      <a-dropdown v-if=\"selectedRowKeys.length > 0\">\n        <a-menu slot=\"overlay\">\n          <a-menu-item key=\"1\" @click=\"batchDel\"><a-icon type=\"delete\"/>删除</a-menu-item>\n        </a-menu>\n        <a-button style=\"margin-left: 8px\"> 批量操作 <a-icon type=\"down\" /></a-button>\n      </a-dropdown>\n    </div>\n\n    <!-- table区域-begin -->\n    <div>\n      <div class=\"ant-alert ant-alert-info\" style=\"margin-bottom: 16px;\">\n        <i class=\"anticon anticon-info-circle ant-alert-icon\"></i> 已选择 <a style=\"font-weight: 600\">{{ selectedRowKeys.length }}</a>项\n        <a style=\"margin-left: 24px\" @click=\"onClearSelected\">清空</a>\n      </div>\n\n      <a-table\n        ref=\"table\"\n        size=\"middle\"\n        bordered\n        rowKey=\"id\"\n        :columns=\"columns\"\n        :dataSource=\"dataSource\"\n        :pagination=\"ipagination\"\n        :loading=\"loading\"\n        :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\n        @change=\"handleTableChange\">\n\n        <span slot=\"action\" slot-scope=\"text, record\">\n          <a @click=\"handleEdit(record)\">编辑</a>\n\n          <a-divider type=\"vertical\" />\n          <a-dropdown>\n            <a class=\"ant-dropdown-link\">更多 <a-icon type=\"down\" /></a>\n            <a-menu slot=\"overlay\">\n              <a-menu-item>\n                <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record.id)\">\n                  <a>删除</a>\n                </a-popconfirm>\n              </a-menu-item>\n            </a-menu>\n          </a-dropdown>\n        </span>\n\n      </a-table>\n    </div>\n    <!-- table区域-end -->\n\n    <!-- 表单区域 -->\n    <productForward-modal ref=\"modalForm\" @ok=\"modalFormOk\"></productForward-modal>\n  </a-card>\n</template>\n\n<script>\n  import ProductForwardModal from './modules/ProductForwardModal'\n  import { JeecgListMixin } from '@/mixins/JeecgListMixin'\n\n  export default {\n    name: \"ProductForwardList\",\n    mixins:[JeecgListMixin],\n    components: {\n      ProductForwardModal\n    },\n    data () {\n      return {\n        description: '产品转发表管理页面',\n        // 表头\n        columns: [\n          {\n            title: '#',\n            dataIndex: '',\n            key:'rowIndex',\n            width:60,\n            align:\"center\",\n            customRender:function (t,r,index) {\n              return parseInt(index)+1;\n            }\n           },\n\t\t   {\n            title: '当前登陆的用户ID',\n            align:\"center\",\n            dataIndex: 'userid'\n           },\n\t\t   {\n            title: '转发的用户ID',\n            align:\"center\",\n            dataIndex: 'forwarduserid'\n           },\n\t\t   {\n            title: '产品ID',\n            align:\"center\",\n            dataIndex: 'productid'\n           },\n\t\t   {\n            title: '创建时间',\n            align:\"center\",\n            dataIndex: 'createtime'\n           },\n\t\t   {\n            title: '类型（0转发注册1转发进入）',\n            align:\"center\",\n            dataIndex: 'type'\n           },\n          {\n            title: '操作',\n            dataIndex: 'action',\n            align:\"center\",\n            scopedSlots: { customRender: 'action' },\n          }\n        ],\n\t\turl: {\n          list: \"/product/productForward/list\",\n          delete: \"/product/productForward/delete\",\n          deleteBatch: \"/product/productForward/deleteBatch\",\n          exportXlsUrl: \"product/productForward/exportXls\",\n          importExcelUrl: \"product/productForward/importExcel\",\n       },\n    }\n  },\n  computed: {\n    importExcelUrl: function(){\n      return `${window._CONFIG['domianURL']}/${this.url.importExcelUrl}`;\n    }\n  },\n    methods: {\n     \n    }\n  }\n</script>\n<style scoped>\n  @import '~@assets/less/common.less'\n</style>"]}]}