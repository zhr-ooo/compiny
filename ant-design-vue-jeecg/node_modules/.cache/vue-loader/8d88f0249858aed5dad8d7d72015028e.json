AACA;AACA","file":"Logo.vue","sourceRoot":"src/components/tools","sourcesContent":["<template>\n  <div class=\"logo\">\n    <router-link :to=\"{name:'dashboard'}\">\n\n      <!-- update-begin- author:sunjianlei --- date:20190814 --- for: logo颜色根据主题颜色变化 -->\n      <img v-if=\"navTheme === 'dark'\" src=\"~@/assets/logoo.png\" alt=\"logo\">\n      <img v-else src=\"~@/assets/logoo.png\" alt=\"logo\">\n      <!-- update-begin- author:sunjianlei --- date:20190814 --- for: logo颜色根据主题颜色变化 -->\n\n      <h1 v-if=\"showTitle\">{{ title }}</h1>\n    </router-link>\n  </div>\n</template>\n\n<script>\n  import { mixin } from '@/utils/mixin.js'\n\n  export default {\n    name: 'Logo',\n    mixins: [mixin],\n    props: {\n      title: {\n        type: String,\n        default: 'V云空间',\n        required: false\n      },\n      showTitle: {\n        type: Boolean,\n        default: true,\n        required: false\n      }\n    }\n  }\n</script>\n<style lang=\"scss\" scoped>\n  /*缩小首页布 局顶部的高度*/\n  $height: 59px;\n\n  .sider {\n    box-shadow: none !important;\n    .logo {\n      height: $height !important;\n      line-height: $height !important;\n      box-shadow: none !important;\n      transition: background 300ms;\n\n      a {\n        color: white;\n        &:hover {\n          color: rgba(255, 255, 255, 0.8);\n        }\n      }\n    }\n\n    &.light .logo {\n      background-color: #1890ff;\n    }\n  }\n</style>\n"]}]}